		<!-- Bomb release -->
		
		<crash>
			<name>Bomb_on_hardpoint3</name>
			
			<condition>
				<equals>
					<property>controls/armament/station[2]/release</property>
					<value>true</value>
				</equals>
			</condition>
			
			<path>Aircraft/Su-25/Sounds/Samples/Bomb.wav</path>
		</crash>
		
		<crash>
			<name>Bomb_on_hardpoint8</name>
			
			<condition>
				<equals>
					<property>controls/armament/station[7]/release</property>
					<value>true</value>
				</equals>
			</condition>
			
			<path>Aircraft/Su-25/Sounds/Samples/Bomb.wav</path>
		</crash>
		
		<!-- Tank release -->
		
		<crash>
			<name>Tank 1</name>
			
			<condition>
				<property>controls/armament/station3/release</property>
				<value>true</value>
			</condition>
			
			<path>Aircraft/Su-25/Sounds/Samples/Bomb.wav</path>
		</crash>
		
		<crash>
			<name>Tank 2</name>
			
			<condition>
				<property>controls/armament/station4/release</property>
				<value>true</value>
			</condition>
			
			<path>Aircraft/Su-25/Sounds/Samples/Bomb.wav</path>
		</crash>
		
		<crash>
			<name>Tank 3</name>
			
			<condition>
				<property>controls/armament/station5/release</property>
				<value>true</value>
			</condition>
			
			<path>Aircraft/Su-25/Sounds/Samples/Bomb.wav</path>
		</crash>
		
		<crash>
			<name>Tank 4</name>
			
			<condition>
				<property>controls/armament/station6/release</property>
				<value>true</value>
			</condition>
			
			<path>Aircraft/Su-25/Sounds/Samples/Bomb.wav</path>
		</crash>
		
		<!-- Flare/Chaff release -->
		
		<flare>
			<name>Flares</name>
			
			<condition>
				<property>controls/armament/trigger2</property>
			</condition>
			
			<path>Aircraft/Su-25/Sounds/Samples/Flare.wav</path>
		</flare>
		
		<!-- Stall warning -->
		
		<stall>
			<name>Stall</name>
			<mode>looped</mode>
			<path>Aircraft/Su-25/Sounds/Samples/Stall.wav</path>
			
			<condition>
				<property>/sim/current-view/internal</property>
				<less-than>
					<property>velocities/airspeed-kt</property>
					<value>175</value>
				</less-than>
				<not>
					<property>/gear/gear[0]/wow</property>
					<property>/gear/gear[1]/wow</property>
					<property>/gear/gear[2]/wow</property>
					<property>/surface-positions/flap-pos-norm</property>
					<property>/gear/gear[0]/position-norm</property>
				</not>
			</condition>
			
			<volume>
				<max>0.5</max>
			</volume>
			
			<position>
				<x>-23</x>
				<y>0</y>
				<z>1</z>
			</position>
			
			<reference-dist>1.0</reference-dist>
			<max-dist>2.0</max-dist>
		</stall>
		
		<stall>
			<name>Stall</name>
			<mode>looped</mode>
			<path>Aircraft/Su-25/Sounds/Samples/Stall.wav</path>
			
			<condition>
				<property>/sim/current-view/internal</property>
				<less-than>
					<property>velocities/airspeed-kt</property>
					<value>150</value>
				</less-than>
				<not>
					<property>/gear/gear[0]/wow</property>
					<property>/gear/gear[1]/wow</property>
					<property>/gear/gear[2]/wow</property>
					<property>/gear/gear[0]/position-norm</property>
				</not>
			</condition>
			
			<volume>
				<max>0.5</max>
			</volume>
			
			<position>
				<x>-22</x>
				<y>0</y>
				<z>0</z>
			</position>
			
			<reference-dist>1.0</reference-dist>
			<max-dist>2.0</max-dist>
		</stall>
		
		<stall>
			<name>Stall</name>
			<mode>looped</mode>
			<path>Aircraft/Su-25/Sounds/Samples/Stall.wav</path>
			
			<condition>
				<property>/sim/current-view/internal</property>
				<less-than>
					<property>velocities/airspeed-kt</property>
					<value>140</value>
				</less-than>
				<not>
					<property>/gear/gear[0]/wow</property>
					<property>/gear/gear[1]/wow</property>
					<property>/gear/gear[2]/wow</property>
					<property>/surface-positions/flap-pos-norm</property>
				</not>
			</condition>
			
			<volume>
				<max>0.5</max>
			</volume>
			
			<position>
				<x>-22</x>
				<y>0</y>
				<z>0</z>
			</position>
			
			<reference-dist>1.0</reference-dist>
			<max-dist>2.0</max-dist>
		</stall>
		
		<!-- Aircraft crash -->
		
		<crash>
			<name>Crash</name>
			<condition>
				<equals>
					<property>sim/crashed</property>
					<value type="bool">true</value>
				</equals>
			</condition>
			<path>Sounds/crash.wav</path>
		</crash>
		
		<!-- Fly-By -->
		
		<flyby>
			<name>Fly-By</name>
			<mode>looped</mode>
			<path>Aircraft/Su-25/Sounds/Samples/Fly-By.wav</path>
			
			<condition>
				<equals>
					<property>sim/current-view/view-number</property>
					<value>6</value>
				</equals>
				<not>
					<property>/sim/current-view/internal</property>
					<property>/engines/engine/out-of-fuel</property>
				</not>
			</condition>
			
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>0.001</factor>
			</volume>
			
		</flyby>
		
		<!-- Enviroment -->
		
		<wind>
			<name>Wind</name>
			<mode>looped</mode>
			<path>Sounds/wind.wav</path>
			
			<condition>
				<not>
					<property>/sim/current-view/internal</property>
				</not>
			</condition>
			
			<property>/velocities/airspeed-kt</property>
			
			<volume>
				<property>/position/altitude-ft</property>
				<factor>-0.000015</factor>
				<offset>1.0</offset>
				<min>0.1</min>
				<max>0.7</max>
			</volume>
			
			<volume>
				<property>/velocities/airspeed-kt</property>
				<factor>0.015</factor>
				<min>0.03</min>
				<max>0.55</max>
			</volume>
			
			<pitch>
				<property>/velocities/airspeed-kt</property>
				<factor>0.01</factor>
				<offset>0.75</offset>
			</pitch>
			
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</wind>
		
	</fx>
	
</PropertyList>
