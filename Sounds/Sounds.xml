<?xml version="1.0"?>

<PropertyList>

	<fx>
	
	<!-- Engines -->
	
		<engine>
			<name>Turbine</name>
			<mode>looped</mode>
			<path>Aircraft/Su-25/Sounds/Samples/Engine-Turbine.wav</path>
			
			<condition>
				<property>/engines/engine/running</property>
				<not>
					<property>sim/crashed</property>
					<value type="bool">true</value>
					<property>/sim/current-view/internal</property>
				</not>
			</condition>
			
			<orientation>
				<x>0</x>
				<y>0</y>
				<z>180</z>
				<inner-cone>180.0</inner-cone>
				<outer-cone>360.0</outer-cone>
				<outer-gain>0.2</outer-gain>
			</orientation>
			
			<volume>
				<property>engines/engine/n2</property>
				<factor>0.09</factor>
				<min>0.3</min>
				<max>0.9</max>
			</volume>
			
			<pitch>
				<property>engines/engine/n2</property>
				<factor>0.01</factor>
				<min>0</min>
				<max>0.9</max>
			</pitch>
			
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</engine>
		
		<engine>
			<name>Turbine Interior</name>
			<mode>looped</mode>
			<path>Aircraft/Su-25/Sounds/Samples/Engine-Turbine.wav</path>
			
			<condition>
				<and>
					<property>/engines/engine/running</property>
					<property>/sim/current-view/internal</property>
				</and>
				<not>
					<property>sim/crashed</property>
					<value type="bool">true</value>
				</not>
			</condition>
			
			<orientation>
				<x>0</x>
				<y>0</y>
				<z>180</z>
				<inner-cone>180.0</inner-cone>
				<outer-cone>360.0</outer-cone>
				<outer-gain>0.2</outer-gain>
			</orientation>
			
			<volume>
				<property>engines/engine/n2</property>
				<factor>0.01</factor>
				<min>0.01</min>
				<max>0.05</max>
			</volume>
			
			<pitch>
				<property>engines/engine/n2</property>
				<factor>0.01</factor>
				<min>0</min>
				<max>0.9</max>
			</pitch>
			
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</engine>
		
		<engine>
			<name>Thrust Low Freq Exterior</name>
			<path>Aircraft/Su-25/Sounds/Samples/Engine-Low-Freq.wav</path>
			<mode>looped</mode>
			
			<condition>
				<property>/engines/engine/running</property>
				<not>
					<property>/sim/current-view/internal</property>
					<property>sim/crashed</property>
					<value type="bool">true</value>
				</not>
			</condition>
			
			<property>engines/engine/thrust-lbs</property>
			
			<volume>
				<property>engines/engine/thrust-lbs</property>
				<factor>0.008</factor>
				<min>0</min>
				<max>4.9</max>
			</volume>
			
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</engine>
		
		<engine>
			<name>Thrust Low Freq Interior</name>
			<path>Aircraft/Su-25/Sounds/Samples/Engine-Low-Freq.wav</path>
			<mode>looped</mode>
			
			<condition>
				<and>
					<property>/engines/engine/running</property>
					<property>/sim/current-view/internal</property>
				</and>
				<not>
					<property>sim/crashed</property>
					<value type="bool">true</value>
				</not>
			</condition>
			
			<property>engines/engine/thrust-lbs</property>
			
			<volume>
				<property>engines/engine/thrust-lbs</property>
				<factor>0.00003</factor>
				<min>0</min>
				<max>2.9</max>
			</volume>
			
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</engine>
		
		<engine>
			<name>Burner Low Freq</name>
			<path>Aircraft/Su-25/Sounds/Samples/Burner-Low-Freq.wav</path>
			<mode>once</mode>
			
			<condition>
				<property>controls/engines/engine/reheat</property>
				<not>
					<property>sim/crashed</property>
					<value type="bool">true</value>
				</not>
			</condition>
			
			<volume>
				<offset>4.9</offset>
			</volume>
			
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</engine>
		
		<engine>
			<name>Burner Low Freq Loop</name>
			<path>Aircraft/Su-25/Sounds/Samples/Engine-Low-Freq.wav</path>
			<mode>looped</mode>
			
			<condition>
				<property>controls/engines/engine/reheat</property>
				<not>
					<property>sim/crashed</property>
					<value type="bool">true</value>
				</not>
			</condition>
			
			<volume>
				<offset>-0.2</offset>
			</volume>
			
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</engine>
		
		<engine>
			<name>Burner Loop</name>
			<path>Aircraft/Su-25/Sounds/Samples/Reheat.wav</path>
			<mode>looped</mode>
			
			<condition>
				<property>/engines/engine/running</property>
				<not>
					<property>/sim/current-view/internal</property>
					<property>sim/crashed</property>
					<value type="bool">true</value>
				</not>
			</condition>
			
			<volume>
				<property>controls/engines/engine/reheat</property>
				<factor>2</factor>
				<min>0</min>
				<max>4.5</max>
			</volume>
			
			<orientation>
				<x>0.0</x>
				<y>0.0</y>
				<z>180.0</z>
				<inner-cone>180.0</inner-cone>
				<outer-cone>360.0</outer-cone>
				<outer-gain>0.2</outer-gain>
			</orientation>
			
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</engine>
		
		<!-- Pilot -->
		
		<voice>
			<name>G-Force Breath Vox</name>
			<path>Aircraft/Su-25/Sounds/Samples/Breath.wav</path>
			
			<condition>
				<and>
					<property>/sim/current-view/internal</property>
					<greater-than>
						<property>/accelerations/pilot/z-accel-fps_sec</property>
						<value>-161</value>
					</greater-than>
				</and>
			</condition>
			
			<volume>
				<factor>0.9</factor>
			</volume>
		</voice>
		
		<voice>
			<name>G-Force Vox</name>
			<path>Aircraft/Su-25/Sounds/Samples/G-Force.wav</path>
			
			<condition>
				<and>
					<property>/sim/current-view/internal</property>
					<less-than>
						<property>/accelerations/pilot/z-accel-fps_sec</property>
						<value>-225</value>
					</less-than>
				</and>
			</condition>
			
			<volume>
				<factor>0.9</factor>
			</volume>
		</voice>
		
		<!-- Flaps -->
		
		<flap>
			<name>Flaps</name>
			<mode>in-transit</mode>
			<path>Sounds/flaps.wav</path>
			<property>/surface-positions/flap-pos-norm</property>
			
			<volume>
				<max>0.25</max>
			</volume>
			
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</flap>
		
		<!-- Landing Gear -->
		
		<rumble>
			<name>Rumble</name>
			<mode>looped</mode>
			<path>Sounds/rumble.wav</path>
			
			<condition>
				<or>
					<property>/gear/gear[0]/wow</property>
					<property>/gear/gear[1]/wow</property>
					<property>/gear/gear[2]/wow</property>
				</or>
			</condition>
			
			<volume>
				<property>/velocities/airspeed-kt</property>
				<factor>.0005</factor>
			</volume>
			
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</rumble>
		
		<gear>
			<name>Gear</name>
			<mode>in-transit</mode>
			<path>Sounds/gear.wav</path>
			<property>/gear/gear[0]/position-norm</property>
			
			<pitch>
				<internal>dt_play</internal>
				<offset>1.0</offset>
				<factor>-0.001</factor>
			</pitch>
			
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</gear>
		
		<gear>
			<name>Lock</name>
			<path>Sounds/gear-lck.wav</path>
			
			<condition>
				<equals>
					<property>/gear/gear[0]/position-norm</property>
					<value>0</value>
				</equals>
			</condition>
			
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</gear>
		
		<squeal>
			<name>Squeal</name>
			<path>Sounds/squeal.wav</path>
			
			<condition>
				<or>
					<property>/gear/gear[0]/wow</property>
					<property>/gear/gear[1]/wow</property>
					<property>/gear/gear[2]/wow</property>
				</or>
			</condition>
			
			<volume>
				<property>/velocities/speed-down-fps</property>
				<factor>0.05</factor>
			</volume>
			<volume>
				<property>/velocities/airspeed-kt</property>
				<factor>0.01</factor>
			</volume>
			
			<volume>
				<internal>dt_stop</internal>
				<factor>0.05</factor>
				<max>1.0</max>
			</volume>
			
			<pitch>
				<property>/velocities/airspeed-kt</property>
				<factor>0.0025</factor>
				<offset>1.2</offset>
			</pitch>
			
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</squeal>
		
		<!-- Weapons -->
		
		<!-- Gun fire -->
		
		<guns>
			<name>Gun</name>
			<mode>looped</mode>
			<path>Aircraft/Su-25/Sounds/Samples/Gun.wav</path>
			
			<condition>
				<property>controls/armament/trigger</property>
			</condition>
			
			<volume>
				<factor>1.2</factor>
				<max>1.0</max>
			</volume>
			
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</guns>
		
		<!-- Missile fire -->
		
		<crash>
			<name>Missile 1</name>
			
			<condition>
				<property>controls/armament/station0/release</property>
				<value>true</value>
			</condition>
			
			<path>Aircraft/Su-25/Sounds/Samples/Missile.wav</path>
		</crash>
		
		<crash>
			<name>Missile 2</name>
			
			<condition>
				<property>controls/armament/station9/release</property>
				<value>true</value>
			</condition>
			
			<path>Aircraft/Su-25/Sounds/Samples/Missile.wav</path>
		</crash>
		
