<?xml version="1.0"?>
<PropertyList>

    <path>Clock.ac</path>

    <animation>
        <type>material</type>
        <object-name>face</object-name>
        <object-name>sec</object-name>
        <object-name>min</object-name>
        <object-name>hour</object-name>
        <emission>
            <red>0.8</red>
            <green>0.8</green>
            <blue>0.8</blue>
        </emission> 
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>sec</object-name>
        <property>/instrumentation/clock/indicated-sec</property>
        <factor>6</factor>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>min</object-name>
        <property>/instrumentation/clock/indicated-sec</property>
        <factor>0.1</factor>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0.0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>hour</object-name>
        <property>/instrumentation/clock/indicated-sec</property>
        <factor>0.008333</factor>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0.0</z>
        </axis>
    </animation>

</PropertyList>