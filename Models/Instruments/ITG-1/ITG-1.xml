<?xml version="1.0"?>

<PropertyList>

<path>ITG-1.ac</path>

<animation>
 <type>material</type>
 <object-name>ITG-1.needle</object-name>
 <object-name>ITG-1.face</object-name>
 <emission>
  <red>0.8</red>
  <green>0.4</green>
  <blue>0.2</blue>
  <factor-prop>mig29/controls/lighting/instruments-norm</factor-prop>
 </emission>
</animation>

<animation>
 <type>rotate</type>
 <object-name>ITG-1.needle</object-name>
 <property>/engines/engine[0]/egt_degf</property>
 <interpolation>
  <entry><ind> 392</ind><dep>  0</dep></entry>
  <entry><ind>2012</ind><dep>230</dep></entry>
 </interpolation>
 <axis>
  <x>-1</x>
  <y> 0</y>
  <z> 0</z>
 </axis>
</animation>

</PropertyList>
