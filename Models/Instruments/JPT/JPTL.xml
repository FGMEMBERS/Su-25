<?xml version="1.0"?>

<PropertyList>

	<path>JPT.ac</path>

	<animation>
		<type>material</type>
		<object-name>face</object-name>
		<object-name>needle</object-name>
		<emission>
			<red>0.8</red>
			<green>0.8</green>
			<blue>0.8</blue>
		</emission>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>needle</object-name>
		<property>/engines/engine[0]/egt-degf</property>
		<interpolation>
			<entry><ind>0.0</ind><dep>0.0</dep></entry>
			<entry><ind>392.0</ind><dep>50.0</dep></entry>
			<entry><ind>1112.0</ind><dep>145.0</dep></entry>
			<entry><ind>1832.0</ind><dep>247.0</dep></entry>
		</interpolation>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>  
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

</PropertyList>
