<?xml version="1.0"?>

<PropertyList>

	<path>Machometer.ac</path>

	<animation>
		<type>material</type>
		<object-name>needle1</object-name>
		<object-name>needle2</object-name>
		<object-name>index</object-name>
		<object-name>face</object-name>
		<emission>
			<red>0.8</red>
			<green>0.8</green>
			<blue>0.8</blue>
		</emission>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>needle1</object-name>
		<property>velocities/mach</property>
		<interpolation>
			<entry><ind>0.0</ind><dep>  0</dep></entry>
			<entry><ind>3.0</ind><dep>360</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y> 0</y>
			<z> 0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>needle2</object-name>
		<property>velocities/airspeed-kt</property>
		<interpolation>
			<entry><ind>   0</ind><dep>  0</dep></entry>
			<entry><ind>3000</ind><dep>360</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y> 0</y>
			<z> 0</z>
		</axis>
	</animation>

</PropertyList>