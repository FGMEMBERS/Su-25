</animation>

<animation>
 <type>rotate</type>
 <object-name>IKG-1.needle1</object-name>
 <!--property>mig29/instrumentation/IKG/general</property-->
 <property>fdm/jsbsim/hs/hs1-pressure</property>
 <interpolation>
  <entry><ind>  0</ind><dep> 0.0</dep></entry>
  <entry><ind>210</ind><dep>36.8</dep></entry>
 </interpolation>
 <center>
  <x-m>-0.06</x-m>
  <y-m> 0.00</y-m>
  <z-m> 0.00</z-m>
 </center>
 <axis>
  <x> 0</x>
  <y>-1</y>
  <z> 0</z>
 </axis>
</animation>

