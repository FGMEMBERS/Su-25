<animation>
 <type>select</type>
 <object-name>IP-52.cp</object-name>
 <condition>
  <and>
   <greater-than>
    <property>mig29/instrumentation/electrical/v27</property>
    <value>18</value>
   </greater-than>
   <or>
    <equals>
     <property>mig29/controls/lighting/kontr_lamp</property>
     <value>1</value>
    </equals>
    <equals>
     <property>mig29/instrumentation/IP-52/rl</property>
     <value>1</value>
    </equals>
   </or>
  </and>
 </condition>
</animation>

